{
  "name": "regular-income-gui",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint",
    "prepack": "rm -f regular-income-gui-*.tgz",
    "postpack": "docker build -t 192.168.1.2:5000/regular-income-frontend:latest . && docker push 192.168.1.2:5000/regular-income-frontend:latest"
  },
  "dependencies": {
    "@amcharts/amcharts4": "4.7.18",
    "@amcharts/amcharts4-geodata": "4.1.9",
    "axios": "0.19.0",
    "bootstrap": "4.4.1",
    "bootstrap-vue": "2.1.0",
    "core-js": "3.6.1",
    "moment": "2.24.0",
    "npm": "6.13.4",
    "uuid": "3.3.3",
    "vue": "2.6.11",
    "vue-router": "3.1.3",
    "vuelidate": "0.7.4"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "4.1.2",
    "@vue/cli-plugin-eslint": "4.1.2",
    "@vue/cli-service": "4.1.2",
    "babel-eslint": "10.0.3",
    "eslint": "6.8.0",
    "eslint-plugin-vue": "6.1.2",
    "vue-template-compiler": "2.6.11"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "rules": {},
    "parserOptions": {
      "parser": "babel-eslint"
    }
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions"
  ]

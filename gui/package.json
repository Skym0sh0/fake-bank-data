{
  "name": "regular-income-gui",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "serve": "set NODE_OPTIONS=--openssl-legacy-provider && vue-cli-service serve",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint",
    "prebuild": "rimraf ./dist",
    "postbuild": "docker build -t 192.168.1.2:5000/regular-income-frontend:latest . && docker push 192.168.1.2:5000/regular-income-frontend:latest"
  },
  "dependencies": {
    "@amcharts/amcharts4": "4.10.38",
    "@amcharts/amcharts4-geodata": "4.1.28",
    "@riophae/vue-treeselect": "0.4.0",
    "axios": "1.6.5",
    "bootstrap": "4.6.2",
    "bootstrap-vue": "2.23.1",
    "core-js": "3.6.2",
    "lodash": "4.17.21",
    "moment": "2.30.1",
    "npm": "6.13.4",
    "uuid": "3.3.3",
    "vue": "2.6.14",
    "vue-drag-drop": "1.1.4",
    "vue-global-events": "1.1.2",
    "vue-router": "3.1.3",
    "vuelidate": "0.7.4",
    "vuetify": "2.7.1"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "4.1.2",
    "@vue/cli-plugin-eslint": "4.1.2",
    "@vue/cli-service": "4.1.2",
    "babel-eslint": "10.0.3",
    "eslint": "6.8.0",
    "eslint-plugin-vue": "6.1.2",
    "sass": "1.24.2",
    "sass-loader": "8.0.0",
    "vue-cli-plugin-vuetify": "2.0.3",
    "vue-template-compiler": "2.6.14",
    "vuetify-loader": "1.4.3"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "rules": {},
    "parserOptions": {
      "parser": "babel-eslint"
    }
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions"
  ]
}
